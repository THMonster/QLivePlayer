{
    "manifest_version": 2,

    "name": "Open With DMLive",
    "description": "Open With DMLive",
    "version": "0.3.0",

    "author": "THMonster <me@thmonster.com>",

    "icons": {
        "48": "icons/revda.png"
    },

    "background": {
        "scripts": [
            "extension.js"
        ],
        "persistent": false
